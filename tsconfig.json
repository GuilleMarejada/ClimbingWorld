{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@image/*": ["src/image/*"]
    },
    "types": ["astro/client"],
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "globals.d.ts"]
}